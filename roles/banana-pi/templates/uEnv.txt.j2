bootargs=root=/dev/mmcblk0p2 rootfstype=ext4 rootwait relatime elevator=deadline console=tty0 disp.screen0_output_mode=EDID:1920x1080p60 hdmi.audio=EDID:0 console=ttyS0,115200 sunxi_fb_mem_reserve=16 sunxi_g2d_mem_reserve=0 sunxi_ve_mem_reserve=0 cma=16M
aload_script=fatload mmc 0 0x43000000 script.bin;
aload_kernel=fatload mmc 0 0x48000000 uImage; bootm 0x48000000;
uenvcmd=run aload_script aload_kernel
